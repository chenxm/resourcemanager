{
  "name": "egret-resource-manager",
  "version": "5.0.0-19",
  "bin": {
    "res": "script/out/cli.js"
  },
  "main": "script/out/index.js",
  "typings": "script/out/index.d.ts",
  "description": "",
  "scripts": {
    "res": "node script/out/cli.js build",
    "tsc": "tsc && uglifyjs bin/resourcemanager/resourcemanager.js > bin/resourcemanager/resourcemanager.min.js",
    "tsc-pure": "tsc -p tsconfig-pure.json && uglifyjs bin/resourcemanager-pure/resourcemanager-pure.js > bin/resourcemanager-pure/resourcemanager-pure.min.js",
    "debug": "tsc -p script && tsc && egret build test/es6-api -e && npm run res test/es6-api",
    "test": "tsc && egret clean test/promise-api && res test/promise-api && egret run test/promise-api -a",
    "debug-classic": "tsc -p script && tsc && egret build test/classic-api -e && npm run res test/classic-api"
  },
  "author": "wander",
  "license": "ISC",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "dependencies": {
    "crc32": "^0.2.2",
    "egret-node-utils": "0.0.7",
    "egret-typescript-ast": "0.0.11",
    "fs-extra-promise": "0.4.1",
    "get-stream": "^3.0.0",
    "htmlparser2": "^3.9.2",
    "iconv-lite": "^0.4.18",
    "map-stream": "0.0.6",
    "typescript": "2.1.6",
    "vinyl": "^2.0.1",
    "vinyl-fs": "^2.4.4",
    "watch": "^1.0.2",
    "yazl": "^2.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.0",
    "@types/fs-extra": "2.0.0",
    "@types/fs-extra-promise": "0.0.31",
    "@types/htmlparser2": "^3.7.29",
    "@types/node": "^7.0.8",
    "@types/vinyl": "^2.0.0",
    "@types/vinyl-fs": "^2.4.1",
    "@types/watch": "0.0.28",
    "uglify-js": "^3.0.28"
  }
}