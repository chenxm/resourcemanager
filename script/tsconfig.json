{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "out",
        "declaration": true,
        "allowUnreachableCode": true,
        "strictNullChecks": true,
        "newLine": "LF",
        "lib": [
            "es5",
            "webworker",
            "es2015.promise"
        ]
    },
    "exclude": [
        "node_modules",
        "out"
    ]
}